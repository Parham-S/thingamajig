{
  "env": {
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": [["@babel/plugin-transform-runtime", { "regenerator": true }]]
    },
    "production": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": [["@babel/plugin-transform-runtime", { "regenerator": true }]]
    },
    "development": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": [
        "react-refresh/babel",
        ["@babel/plugin-transform-runtime", { "regenerator": true }]
      ]
    }
  }
}
