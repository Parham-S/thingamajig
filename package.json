{
  "name": "sample-fullstack-hooks-asyncawait",
  "version": "1.0.0",
  "main": "server/built/index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn clean && cross-env NODE_ENV=production yarn build:client && cross-env NODE_ENV=production node ./server/index.js",
    "build:ts": "yarn tsc -p tsconfig.server.json --watch",
    "build:client": "webpack --mode production",
    "clean": "rm -rf ./dist && mkdir dist",
    "db:migrate": "yarn knex migrate:latest --knexfile server/db/knexfile.js",
    "db:rollback": "yarn knex migrate:rollback --knexfile server/db/knexfile.js",
    "db:setup": "yarn db:rollback && yarn db:migrate",
    "dev:client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "dev:server": "nodemon ./server/built/index.js",
    "dev": "concurrently --kill-others-on-fail \"yarn run build:ts\" \"yarn run dev:server\" \"yarn run dev:client\"",
    "lint": "eslint ./server --fix",
    "test": "jest ./server/src/__tests__",
    "test:watch": "jest --watch --notify --notifyMode=change ./server/src/__tests__/"
  },
  "proxy": "http://localhost:8080",
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.14",
    "knex-tiny-logger": "^2.1.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "passport": "^0.5.0",
    "passport-github": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.2",
    "react-router-dom": "^6.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.0",
    "@types/passport-github": "^1.1.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.3",
    "jest-extended": "^1.2.0",
    "nodemon": "^2.0.15",
    "react-refresh": "^0.11.0",
    "style-loader": "^3.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
}
