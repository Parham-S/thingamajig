{
  "name": "sample-fullstack-hooks-asyncawait",
  "version": "1.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "db:migrate": "yarn knex migrate:latest --knexfile server/db/knexfile.js",
    "db:rollback": "yarn knex migrate:rollback --knexfile server/db/knexfile.js",
    "server": "nodemon ./server/index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "test": "jest ./server/__tests__/test.js",
    "test:watch": "jest --watch --notify --notifyMode=change ./server/__tests__/test.js"
  },
  "proxy": "http://localhost:8080",
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.14",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "passport": "^0.5.0",
    "passport-github": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.2",
    "react-router-dom": "^6.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.4.0",
    "css-loader": "^6.5.1",
    "dotenv": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.3",
    "jest-extended": "^1.2.0",
    "nodemon": "^2.0.15",
    "react-refresh": "^0.11.0",
    "style-loader": "^3.3.1",
    "supertest": "^6.1.6",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
